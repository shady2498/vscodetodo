{
    "compilerOptions": {
      "target": "es5",
      "module": "ESNext",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {
        "*": ["types/*"]
      },
      "outDir": "dist",
      "sourceMap": true
    },
    "include": ["./**/*"],
    "exclude": ["../node_modules/*"],
  }
  